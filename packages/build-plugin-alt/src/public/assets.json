    },
    {
      "title": "Container",
      "package": "@alife/container",
      "version": "0.3.7",
      "urls": [
        "https://g.alicdn.com/fusion-design/auto-layout/0.3.7/dist/container.css",
        "https://g.alicdn.com/fusion-design/auto-layout/0.3.7/dist/container.js"
      ],
